/** Setup the input buffer state to scan the given bytes. The next call to lpt_yylex() will
 * scan from a @e copy of @a bytes.
 * @param bytes the byte buffer to scan
 * @param len the number of bytes in the buffer pointed to by @a bytes.
 * @param lpt_yyscanner The scanner object.
 * @return the newly allocated buffer state object.
 */
YY_BUFFER_STATE lpt_yy_scan_bytes  (lpt_yyconst char * lpt_yybytes, int  _lpt_yybytes_len , lpt_yyscan_t lpt_yyscanner)
{
	YY_BUFFER_STATE b;
	char *buf;
	lpt_yy_size_t n;
	int i;
    
	/* Get memory for full buffer, including space for trailing EOB's. */
	n = _lpt_yybytes_len + 2;
	buf = (char *) lpt_yyalloc(n ,lpt_yyscanner );
	if ( ! buf )
		YY_FATAL_ERROR( "out of dynamic memory in lpt_yy_scan_bytes()" );

	for ( i = 0; i < _lpt_yybytes_len; ++i )
		buf[i] = lpt_yybytes[i];

	buf[_lpt_yybytes_len] = buf[_lpt_yybytes_len+1] = YY_END_OF_BUFFER_CHAR;

	b = lpt_yy_scan_buffer(buf,n ,lpt_yyscanner);
	if ( ! b )
		YY_FATAL_ERROR( "bad buffer in lpt_yy_scan_bytes()" );

	/* It's okay to grow etc. this buffer, and we should throw it
	 * away when we're done.
	 */
	b->lpt_yy_is_our_buffer = 1;

	return b;
}

