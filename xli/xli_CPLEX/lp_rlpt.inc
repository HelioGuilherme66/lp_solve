 */
    void lpt_yy_delete_buffer (YY_BUFFER_STATE  b , lpt_yyscan_t lpt_yyscanner)
{
    struct lpt_yyguts_t * lpt_yyg = (struct lpt_yyguts_t*)lpt_yyscanner;

	if ( ! b )
		return;

	if ( b == YY_CURRENT_BUFFER ) /* Not sure if we should pop here. */
		YY_CURRENT_BUFFER_LVALUE = (YY_BUFFER_STATE) 0;

	if ( b->lpt_yy_is_our_buffer )
		lpt_yyfree((void *) b->lpt_yy_ch_buf ,lpt_yyscanner );

	lpt_yyfree((void *) b ,lpt_yyscanner );
}

