					lpt_yyg->lpt_yy_c_buf_p = lpt_yyg->lpt_yytext_ptr + offset;
					break;
				}
			}
		}

	c = *(unsigned char *) lpt_yyg->lpt_yy_c_buf_p;	/* cast for 8-bit char's */
	*lpt_yyg->lpt_yy_c_buf_p = '\0';	/* preserve lpt_yytext */
