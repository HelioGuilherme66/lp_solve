		if ( c == EOF && ferror( lpt_yyin ) ) \
			YY_FATAL_ERROR( "input in flex scanner failed" ); \
		result = n; \
		} \
	else \
		{ \
		errno=0; \
		while ( (result = fread(buf, 1, max_size, lpt_yyin))==0 && ferror(lpt_yyin)) \
			{ \
			if( errno != EINTR) \
