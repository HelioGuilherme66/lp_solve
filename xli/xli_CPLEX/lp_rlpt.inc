/* User-visible API */

/* lpt_yylex_init is special because it creates the scanner itself, so it is
 * the ONLY reentrant function that doesn't take the scanner as the last argument.
 * That's why we explicitly handle the declaration, instead of using our macros.
 */

int lpt_yylex_init(lpt_yyscan_t* ptr_lpt_yy_globals)

{
    if (ptr_lpt_yy_globals == NULL){
        errno = EINVAL;
        return 1;
    }

    *ptr_lpt_yy_globals = (lpt_yyscan_t) lpt_yyalloc ( sizeof( struct lpt_yyguts_t ), NULL );

    if (*ptr_lpt_yy_globals == NULL){
        errno = ENOMEM;
        return 1;
    }

    /* By setting to 0xAA, we expose bugs in lpt_yy_init_globals. Leave at 0x00 for releases. */
    memset(*ptr_lpt_yy_globals,0x00,sizeof(struct lpt_yyguts_t));

    return lpt_yy_init_globals ( *ptr_lpt_yy_globals );
}

/* lpt_yylex_init_extra has the same functionality as lpt_yylex_init, but follows the
 * convention of taking the scanner as the last argument. Note however, that
 * this is a *pointer* to a scanner, as it will be allocated by this call (and
 * is the reason, too, why this function also must handle its own declaration).
 * The user defined value in the first argument will be available to lpt_yyalloc in
 * the lpt_yyextra field.
 */

int lpt_yylex_init_extra(YY_EXTRA_TYPE lpt_yy_user_defined,lpt_yyscan_t* ptr_lpt_yy_globals )

{
    struct lpt_yyguts_t dummy_lpt_yyguts;

    lpt_yyset_extra (lpt_yy_user_defined, &dummy_lpt_yyguts);

    if (ptr_lpt_yy_globals == NULL){
        errno = EINVAL;
        return 1;
    }
	
    *ptr_lpt_yy_globals = (lpt_yyscan_t) lpt_yyalloc ( sizeof( struct lpt_yyguts_t ), &dummy_lpt_yyguts );
	
    if (*ptr_lpt_yy_globals == NULL){
        errno = ENOMEM;
        return 1;
    }
    
    /* By setting to 0xAA, we expose bugs in
    lpt_yy_init_globals. Leave at 0x00 for releases. */
    memset(*ptr_lpt_yy_globals,0x00,sizeof(struct lpt_yyguts_t));
    
    lpt_yyset_extra (lpt_yy_user_defined, *ptr_lpt_yy_globals);
    
    return lpt_yy_init_globals ( *ptr_lpt_yy_globals );
}

static int lpt_yy_init_globals (lpt_yyscan_t lpt_yyscanner)
{
    struct lpt_yyguts_t * lpt_yyg = (struct lpt_yyguts_t*)lpt_yyscanner;
    /* Initialization is the same as for the non-reentrant scanner.
     * This function is called from lpt_yylex_destroy(), so don't allocate here.
     */

    lpt_yyg->lpt_yy_buffer_stack = 0;
    lpt_yyg->lpt_yy_buffer_stack_top = 0;
    lpt_yyg->lpt_yy_buffer_stack_max = 0;
    lpt_yyg->lpt_yy_c_buf_p = (char *) 0;
    lpt_yyg->lpt_yy_init = 0;
    lpt_yyg->lpt_yy_start = 0;

    lpt_yyg->lpt_yy_start_stack_ptr = 0;
    lpt_yyg->lpt_yy_start_stack_depth = 0;
    lpt_yyg->lpt_yy_start_stack =  NULL;

/* Defined in main.c */
#ifdef YY_STDINIT
    lpt_yyin = stdin;
    lpt_yyout = stdout;
#else
    lpt_yyin = (FILE *) 0;
    lpt_yyout = (FILE *) 0;
#endif

    /* For future reference: Set errno on error, since we are called by
     * lpt_yylex_init()
     */
    return 0;
}

/* lpt_yylex_destroy is for both reentrant and non-reentrant scanners. */
int lpt_yylex_destroy  (lpt_yyscan_t lpt_yyscanner)
{
    struct lpt_yyguts_t * lpt_yyg = (struct lpt_yyguts_t*)lpt_yyscanner;

    /* Pop the buffer stack, destroying each element. */
	while(YY_CURRENT_BUFFER){
		lpt_yy_delete_buffer(YY_CURRENT_BUFFER ,lpt_yyscanner );
		YY_CURRENT_BUFFER_LVALUE = NULL;
		lpt_yypop_buffer_state(lpt_yyscanner);
	}

	/* Destroy the stack itself. */
	lpt_yyfree(lpt_yyg->lpt_yy_buffer_stack ,lpt_yyscanner);
	lpt_yyg->lpt_yy_buffer_stack = NULL;

    /* Destroy the start condition stack. */
        lpt_yyfree(lpt_yyg->lpt_yy_start_stack ,lpt_yyscanner );
        lpt_yyg->lpt_yy_start_stack = NULL;

    /* Reset the globals. This is important in a non-reentrant scanner so the next time
     * lpt_yylex() is called, initialization will occur. */
    lpt_yy_init_globals( lpt_yyscanner);

    /* Destroy the main struct (reentrant only). */
    lpt_yyfree ( lpt_yyscanner , lpt_yyscanner );
    lpt_yyscanner = NULL;
    return 0;
}

